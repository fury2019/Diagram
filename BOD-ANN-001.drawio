<?xml version="1.0" encoding="UTF-8"?>
<uml:UseCaseDiagram xmlns:uml="http://www.omg.org/spec/UML/20090901">
    <actors>
        <actor id="A1" name="마스터 계정" />
        <actor id="A2" name="일반 계정" />
    </actors>
    <usecases>
        <usecase id="UC1" name="공지사항 등록" />
        <usecase id="UC2" name="공지사항 수정" />
        <usecase id="UC3" name="공지사항 삭제" />
        <usecase id="UC4" name="권한 부여" />
        <usecase id="UC5" name="카테고리 설정" />
    </usecases>
    <relationships>
        <association from="A1" to="UC4" />
        <association from="A1" to="UC1" />
        <association from="A1" to="UC2" />
        <association from="A1" to="UC3" />
        <association from="A1" to="UC5" />
        <association from="A2" to="UC1" condition="권한 부여됨" />
        <association from="A2" to="UC2" condition="권한 부여됨" />
        <association from="A2" to="UC3" condition="권한 부여됨" />
        <include from="UC1" to="UC5" />
        <include from="UC2" to="UC5" />
        <extend from="UC1" to="UC4" condition="권한이 없는 경우" />
    </relationships>
</uml:UseCaseDiagram>
